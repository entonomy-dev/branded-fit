# Branded Fit MVP - Environment Configuration
# Copy this file to .env and fill in your API keys

# Server Configuration
PORT=8080
NODE_ENV=development

# Logo Retrieval API - Brandfetch (https://brandfetch.com)
# Get your API key from: https://dashboard.brandfetch.com/api
BRANDFETCH_API_KEY=your_brandfetch_api_key_here

# Alternative: Clearbit Logo API (https://clearbit.com)
# Get your API key from: https://dashboard.clearbit.com/api
CLEARBIT_API_KEY=your_clearbit_api_key_here

# Printify API (https://printify.com)
# Get your API key from: https://printify.com/app/account/api
PRINTIFY_API_KEY=your_printify_api_key_here
PRINTIFY_SHOP_ID=your_printify_shop_id_here

# Shopify API (https://shopify.com)
# Create a private app from: Admin > Settings > Apps and sales channels > Develop apps
SHOPIFY_STORE_URL=your-store.myshopify.com
SHOPIFY_ACCESS_TOKEN=your_shopify_access_token_here
SHOPIFY_API_VERSION=2024-01

# Cache Configuration
CACHE_TTL_SECONDS=3600
CACHE_CHECK_PERIOD_SECONDS=600

# Logging
LOG_LEVEL=info
